{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": "s3:*",
            "Resource": "arn:aws:s3:::example-bucket/*",
            "Condition": {
                "StringEquals": {
                    "aws:SecureTransport": "true",
                    "aws:UserAgent": "aws-sdk-java/1.11.0 AmazonS3EncryptionV2/2.1.3 Linux/4.14.208-160.339.amzn2.x86_64 OpenJDK_64-Bit_Server_VM/25.302-b08 java/1.8.0_302"
                },
                "StringEqualsIfExists": {
                    "aws:UserAgent": "aws-sdk-js/2.971.0 promise"
                },
                "StringEqualsIgnoreCase": {
                    "aws:SourceVpce": "vpce-1234567890abcdef0"
                },
                "StringEqualsIfExists": {
                    "aws:Referer": "https://www.example.com"
                },
                "StringEquals": {
                    "aws:RequestTag/tag-key": "tag-value"
                },
                "StringNotEquals": {
                    "s3:x-amz-acl": "public-read"
                },
                "IpAddress": {
                    "aws:SourceIp": "203.0.113.0/24"
                },
                "StringEquals": {
                    "aws:RequestType": "GetObject"
                }
            }
        }
    ]
}

